#:kivy 1.0.9

<GameOver>:
    id: gameover
    result_button: result_button
    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'center'       
        Button:
            id: result_button
            text: "New game"
            size_hint: [.3, .3]
            on_release: root.manager.current = "menu_screen"